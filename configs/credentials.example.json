{
  "example_credential": {
    "type": "httpBasicAuth",
    "data": {
      "user": "username",
      "password": "password"
    }
  },
  "api_key": {
    "type": "httpHeaderAuth",
    "data": {
      "name": "Authorization",
      "value": "Bearer your_api_key_here"
    }
  },
  "oauth": {
    "type": "oAuth2",
    "data": {
      "grantType": "authorizationCode",
      "accessTokenUrl": "https://example.com/oauth/token",
      "clientId": "your_client_id",
      "clientSecret": "your_client_secret"
    }
  }
}
